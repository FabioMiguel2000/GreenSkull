<lsf>

<!--Initials-->
    <initials>
        <reference length="2" />
        <root id="demoRoot" />

    </initials>
<!-- -->

<!--Cameras-->
    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
        <ortho id="demoOrtho" near="0.2" far="100" left="-0.2" right="0.2" top="0.2" bottom="-0.2">
            <from x="5" y="0" z="10" />
            <to x="5" y="0" z="0" />
            <up x="0" y="1" z="0" />
        </ortho>
    </views>
<!-- -->

<!--Illumination-->
    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />          
        <background r="0.1" g="0.7" b="1.0" a="1.0" />        
    </illumination>
<!-- -->

<!--Lights-->
    <lights>
        
        <light id="demoLight">            
            <enable value="1" />
            <position x="10" y="12" z="9.5" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />                   
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            
        </light>
    </lights>
<!-- -->


<!--Textures-->
    <textures>
        <texture id="ss" path="ss" />
    </textures>
<!-- -->


<!--Materials-->
    <materials>
        <material id="ss">
            <shininess value="ff" />
            <ambient r="ff" g="ff" b="ff" a="ff" />
            <diffuse r="ff" g="ff" b="ff" a="1.0" />
            <specular r="ff" g="ff" b="ff" a="ff" />
            <emissive r="ff" g="ff" b="ff" a="ff" />
        </material>
    </materials>
<!-- -->


<!--Nodes-->
    
    <nodes>

        <!--Root node-->
        <node id="Root"/>
            <material id="ss" />
            <texture id="ss">
                <amplification afs="ff" aft="ff" />
            </texture>

            <transformations>
                <translation x="ff" y="ff" z="ff" />
                <rotation axis="yy" angle="ff" />
                <scale sx="ff" sy="ff" sz="ff" />
            </transformations>

            <descendants>
                <noderef id="Mesa" />
                <noderef id="Caneca" />
                <nodered id="Chao" />
                <nodered id="Luzes" />
                <nodered id="Parede"/>
